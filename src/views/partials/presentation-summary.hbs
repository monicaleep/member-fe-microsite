<section>
  {{#if featured}}
    <h2>Featured Presentation: <a href="/presentations/{{presentation.id}}">{{presentation.topic}}</a></h2>
  {{^}}
    <h1>{{presentation.topic}}</h1>
  {{/if}}
  <img
    src="{{presentation.image}}"
    fetchpriority="high"
    alt="{{presentation.presenter}}"
  />
  <div>{{presentation.presenter}} </div>
  <div>{{presentation.id}}</div>
  <div>{{presentation.tag}}</div>
  {{> favorited_cell favorited=presentation.favorited id=presentation.id }}
</section>
