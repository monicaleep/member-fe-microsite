<tbody id="search-results">

  {{#each presentations}}
    <tr data-testId="presentation-row">
      <td>{{name}}</td>
      <td>{{email}}</td>
      <td>{{firstName}}</td>
      <td>{{lastName}}</td>
      <td><a href="/presentations/{{_id}}">Go to Presentation</a></td>
      {{> favorited_cell favorited=favorited id=_id }}
    </tr>
  {{/each}}
</tbody>
