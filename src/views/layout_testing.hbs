<head><link rel="stylesheet" type="text/css" href="/css/presentation-style.css" /></head>
<body>
    {{#each presentations}}
    <a class="container" href="/presentations/{{id}}">
    <div class="container" >
        <div class="homepage">
            <ul>
                <li>{{ name }}</li>
                <li>{{ firstName }} <li>
                <li>{{ tag }}</li>
            </ul>
        </div>

        <div class="image">
            <img src="{{image}}" fetchpriority="high" alt="{{firstName}} " />
        </div>
    </div>
    </a>
    {{/each}}
</body>
